<body>
  <div id="registerTable">
    <registerTable>      
      <strong>Cadastro de Usu√°rio<br/></strong>
    
    <table class="table">
      <thead>
        <tr>
          <th scope="Id">ID</th>
          <th scope="username">username</th>
          <th scope="email">email</th>
          <th scope="password">senha</th>

        </tr>
      </thead>
      <tbody>
      <% usersList.forEach(dbUsers => { %>
        <tr>
          <th scope="row"><%= dbUsers._id %></th>
          <td><%= dbUsers.username %></td>
          <td><%= dbUsers.email %></td>
          <td><%= dbUsers.password %></td>
          <td class="text-end">
            <div class="d-flex flex-row justify-content-end gap-2">
              <a href="/editUser/<%= dbUsers._id %>" type="button" class="btn btn-secondary btn-small position-relative"><i class="bi bi-pencil"></i></a>
              <form action="/registerTable/<%= dbUsers._id %>?_method=DELETE" method="POST" class="position-relative">
                <button type="submit" class="btn btn-danger btn-small"><i class="bi bi-trash-fill"></i></button>
              </form>
            </div>
          </td>
        </tr>
      <% })%>

        
      </tbody>
     
    </table>
    
  </tabela>
  
  
</div>
</registerTable>


</body>




